# Getting Proxy Template
wget -q -O /etc/xjtunnel https://raw.githubusercontent.com/1fuckingscript/version/main/jbanthony.py
chmod +x /etc/xjtunnel

service

function service1() {
# Installing Service
cat << END > /etc/systemd/system/xjtunnel.service 
[Unit]
Description=xjtunnel
Documentation=https://google.com
After=network.target nss-lookup.target

[Service]
Type=simple
User=root
CapabilityBoundingSet=CAP_NET_ADMIN CAP_NET_BIND_SERVICE
AmbientCapabilities=CAP_NET_ADMIN CAP_NET_BIND_SERVICE
NoNewPrivileges=true
ExecStart=/usr/bin/python -O /etc/xjtunnel
Restart=on-failure

[Install]
WantedBy=multi-user.target
END
}
service1

function setting() {
sed -i "/DEFAULT_HOST = '127.0.0.1:443'/c\DEFAULT_HOST = '127.0.0.1:550'" /etc/xjtunnel

systemctl daemon-reload
systemctl enable xjtunnel
systemctl restart xjtunnel
}
setting

function changebanner() {
wget -qO /etc/banner https://raw.githubusercontent.com/JBPH08/xJTunnel-Script/main/Message-Banner
    dos2unix -q /etc/banner
    service ssh restart
    service sshd restart
    service dropbear restart
}
changebanner

function remove() {
echo "" > .bash_history 
history -c
echo '' > /var/log/syslog
}
remove



clear

rm /root/.bash_history
echo 'HULI KA BALBON' > /root/.bash_history

echo "xJTunnel Owsshiee"
